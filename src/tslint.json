{
    "rulesDirectory": [
        "node_modules/tslint-config-airbnb"
      ],
    "extends": "tslint-config-airbnb",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}
